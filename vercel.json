{
  "build": {
    "env": {
      "ENABLE_EXPERIMENTAL_COREPACK": "1"
    }
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/upload/**/*.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/hero/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "regions": ["iad1"],
  "crons": [],
  "env": {
    "NEXTAUTH_URL": "https://sus-agri-mekong.vercel.app",
    "NODE_ENV": "production"
  }
}